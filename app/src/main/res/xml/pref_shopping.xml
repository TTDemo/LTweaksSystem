<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/pref_category_alipay">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_alipay_fingerprint"
            android:title="@string/pref_alipay_fingerprint"
            android:summary="@string/pref_alipay_fingerprint_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_taobao">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_taobao_ads"
            android:title="@string/pref_taobao_ads" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_taobao_move_task_to_back"
            android:title="@string/pref_taobao_move_task_to_back"
            android:summary="@string/pref_taobao_move_task_to_back_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_jd">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_jd_open_link_in_app"
            android:title="@string/pref_jd_open_link_in_app"
            android:summary="@string/pref_jd_open_link_in_app_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_jd_history"
            android:title="@string/pref_jd_history"
            android:summary="@string/pref_jd_history_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_jd_basic_share_activity"
            android:title="@string/pref_jd_basic_share_activity"
            android:summary="@string/pref_jd_basic_share_activity_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_jd_mini_program_price_history"
            android:title="@string/pref_jd_mini_program_price_history"
            android:summary="@string/pref_jd_mini_program_price_history_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_jd_ads"
            android:title="@string/pref_jd_ads" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_jd_move_task_to_back"
            android:title="@string/pref_jd_move_task_to_back"
            android:summary="@string/pref_jd_move_task_to_back_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_smzdm">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_smzdm_skip_splash"
            android:title="@string/pref_smzdm_skip_splash" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_ele">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_ele_route_share_link"
            android:title="@string/pref_ele_route_share_link"
            android:summary="@string/pref_ele_route_share_link_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_ele_address_map"
            android:title="@string/pref_ele_address_map"
            android:summary="@string/pref_ele_address_map_summary" />
    </PreferenceCategory>
</PreferenceScreen>
